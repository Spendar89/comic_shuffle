<div id="dash_left">
  <div id= "dash_top_comics_div" class = "dash_rank_div">
    <div class ="dash_comics_header">
      Top-Rated:
    </div>
      <div id = "dash_top_comics" class="dash_rank_items">
	<% if !@top_comedians.nil? %>
        <% @top_comedians.each_with_index do |comedian, i| %>
             <%if i < 10 %>

          <div class="dash_comedian_link">
            <%=link_to image_tag(comedian.photo, :class => "dash_comic_image"), comedians_show_path(:id => comedian), :class =>"dash_comic_link" %>
          </div>

          <% end %>
        <% end %>
<%end%>
      </div>
  </div>

  <div id= "dash_recent_comics_div" class="dash_rank_div">
    <div class ="dash_comics_header" id = "dash_recent_comics_header">
      Recently-Added:
    </div>
      <div id = "dash_recent_comics" class="dash_rank_items">
	<% if !@recent_comedians.nil? %>
        <% @recent_comedians.each_with_index do |comedian, i| %>
             <%if i < 10 %>

          <div class="dash_comedian_link">
            <%=link_to image_tag(comedian.photo, :class => "dash_comic_image"), comedians_show_path(:id => comedian), :class =>"dash_comic_link" %>
          </div>

          <% end %>
        <% end %>
	<%end%>
      </div>
  </div>
</div>

<div id="dash_right">
  <div id="dash_random_video_div">
    <%= render :partial => "random_video", :locals=>{:video => @random_video} unless @random_video.nil?%> 
  </div>
</div>

